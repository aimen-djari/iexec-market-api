{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-console": 0,
    "no-param-reassign": [
      "error",
      { "props": true, "ignorePropertyModificationsFor": ["ctx", "ret"] }
    ],
    "no-template-curly-in-string": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "max-classes-per-file": "off",
    "max-len": "off"
  }
}
